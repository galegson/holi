pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                sleep 5
            }
        }
     stage('Build') {
            steps {
                echo 'Hello World'
            }
        }stage('Deploy') {
            steps {
                echo 'Hello World'
            }
        }stage('test') {
            steps {
                echo 'Hello test'
            }
        }   
    }
 }

